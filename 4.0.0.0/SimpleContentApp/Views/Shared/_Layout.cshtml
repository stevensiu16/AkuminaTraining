<html lang="en">
<!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
    <!-- Favicons -->
    <link rel="shortcut icon" href="favicon.ico" />
    <link rel="apple-touch-icon" href="/Content/images/apple-touch-icon.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="/Content/images/apple-touch-icon-72x72.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="/Content/images/apple-touch-icon-114x114.png" />
    <!-- jQuery UI CSS -->
    <link rel="stylesheet" type="text/css" href="/Content/css/jquery-ui-theme/jquery-ui-1.10.4.custom.min.css">
    <!--Google Font-->
    <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600' rel='stylesheet' type='text/css'>
    <!-- Akumina Base CSS -->
    <link rel="stylesheet" type="text/css" href="/Content/css/normalize.css">
    <link rel="stylesheet" type="text/css" href="/Content/css/ak-base.css">
    <link rel="stylesheet" type="text/css" href="/Content/css/magnific-popup.css">
    <link rel="stylesheet" type="text/css" href="/Content/css/ak-content-explorer.css">
    <link rel="stylesheet" type="text/css" href="/Content/css/ak-content-explorer-responsive.css">
    <!-- Modernizr JS -->
    <script src="/Content/js/vendor/jquery.js"></script>
    <script src="/Content/js/vendor/modernizr.js"></script>
    <script src="/Editor/ck/ckeditor.js" type="text/javascript"></script>
</head>
<body>
    <div class="ak-editor">
        <div class="ak-editor-header row">
            <div class="ak-logo-wrapper">
                <div class="ak-logo-emblem"> <a href="@ViewHelperUtility.GetContentAppHomeUrl()"> <img src="/Content/images/ak-logo.png" alt="Akumina Content Explorer" title="Akumina Content Explorer" /> </a> </div>
                <h1 class="ak-logo"><a href="@ViewHelperUtility.GetContentAppHomeUrl()">InterChange SDK Harness</a></h1>
            </div>
            <div class="ak-signed-in-wrapper">
                <div class="ak-signed-in-user">
                    <div class="ak-user-icon"> <img src="/Content/images/ak-default-user.png" alt="[username]" title="[username]" /> </div>
                    <div class="ak-user-name"> Your Name </div>
                    <div class="ak-settings-link"><a href="#"><img src="/Content/images/icon-gear.png" alt="Settings" title="Settings" /></a></div>
                    <a href="#" class="button tiny">Sign Out</a>
                </div>
            </div>
        </div>
        <div class="ak-explorer-wrapper">
            <!-- if the page is 3 columns, it needs the ak-3-col class below, if 2 columns, ak-2-col-->
            <div class="row ak-2-col">
                <div class="ak-left-col ak-left-nav">
                    <div class="ak-left-col-inner">
                        <div class="ak-current-app">
                            <img src="/Content/images/ak-app-banner-white.png" class="ak-app-icon" />
                            <h2 class="ak-app-title">@ViewHelperUtility.GetContentAppTitle()</h2>
                        </div>
                        <div class="ak-list-nav">
                            <ul>
                                @foreach (var nav in @ViewHelperUtility.GetContentAppNavigation())
                                {
                                    <li>
                                        <a href="@nav.Url">@nav.Title</a>
                                    </li>
                                }

                            </ul>
                        </div>
                    </div>
                </div>

                <div class="ak-main-body">
                    <button class="ak-btn ak-off-canvas-trigger"></button>
                    <div class="ak-section-header">
                        <h3>@ViewHelperUtility.GetContentAppTitle()</h3>
                    </div>

                    @Html.Partial("_Header")

                    <div class="ak-main-body-inner">
                        <div class="ak-content-list ak-content-list-icons ak-content-list-info">
                            @RenderBody()
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="ak-copyright">
            &copy;
            <script>document.write(new Date().getFullYear())</script> Akumina, Inc.
        </div>
        <div class="ak-dialog mfp-hide" id="ak-dialog-confirmDelete" title="Delete Instruction Set">
            <div class="ak-edit-content ak-dialog-edit">
                <div class="ak-content-edit-row">
                    <p>Are you sure you want to delete this instruction set? This action cannot be undone and may have potential impact to your site's functionality.</p>
                </div>
                <div class="ak-content-edit-row">
                    <div class="ak-btn-row ak-btn-row-purple">
                        <a href="#" class="ak-btn" id="ak-cancel-delete">Yes</a> <a href="#" class="ak-btn ak-btn-quiet" id="ak-cancel-delete">No</a>
                    </div>
                </div>
            </div>
        </div>


      
        <script src="/Content/js/jquery-ui-1.10.4.custom.min.js"></script>
        <script src="/Content/js/foundation.min.js"></script>
        <script src="/Content/js/jquery-equalHeights.js"></script>
        <script src="/Content/js/vendor/jquery.magnific-popup.js"></script>
        <script src="/Content/js/ak-init.js"></script>
        <script type="text/javascript">
            $(document).ready(function () {
                /* Toggle CSS Class for left nav */
                $('.ak-off-canvas-trigger').click(function () {
                    $('.ak-left-nav').toggleClass('ak-left-nav-expanded');
                    $('.ak-2-col .ak-main-body').toggleClass('ak-push-right');
                });

                /* Popup Modals - NEW for RWD */
                $('#ak-dialog-confirmDelete-btn, #ak-dialog-confirmDelete-btn-2').magnificPopup({
                    type: 'inline',
                    preloader: false,
                    closeBtnInside: true,
                    showCloseBtn: true

                });
                $(document).on('click', '#ak-cancel-delete', function (e) {
                    e.preventDefault();
                    $.magnificPopup.close();
                });

            });
        </script>
</body>
</html>
